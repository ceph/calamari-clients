digraph G {
	manage -> root;
	manage -> osd;
	manage -> pool;
	manage -> logs;
	manage -> first;
	first -> root;
	root -> clusterHosts;
	root -> clusterSettings;
	root -> clusterBrowser;

	osd -> osdHosts;
	pool -> poolNew;
	pool -> poolModify;

    manage [shape=polygon,color="red",sides=4,label="Manage Module"];
