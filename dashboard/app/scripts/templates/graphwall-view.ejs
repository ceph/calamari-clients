    <button type='button' data-id='iops' class='btn btn-primary'>IOPS</button>
    <button type='button' data-id='diskbytes' class='btn btn-primary'>Capacity</button>
    <button type='button' data-id='diskinodes' class='btn btn-primary'>Inodes</button>
</div>
<div class='btn-graph btn-group' data-toggle='buttons-radio'>
    <span class='graph-title'>Network</span>
    <button type='button' data-id='netbytes' class='btn btn-primary'>Bytes</button>
    <button type='button' data-id='netpackets' class='btn btn-primary'>Packets</button>
</div>
